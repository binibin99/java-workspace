package quiz;

public class B10_GuGudan2 {

	// for문으로 출력했던 구구단 2개를 while문으로 출력해보세요

	public static void main(String[] args) {
		int dan = 1;
		int gop = 1;

		while (dan <= 9) {
			dan++;
			System.out.printf("\n%d단 ", ++dan);
			while (gop <= 9) {
				gop++;
				System.out.printf("\n%dx%d = %d\n", dan, gop, dan * gop);

			}

		}

	}
}
